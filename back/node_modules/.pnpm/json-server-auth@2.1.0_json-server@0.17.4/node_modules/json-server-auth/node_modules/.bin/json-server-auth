#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/zTTz1/Desktop/Projects/crm-auth/back/node_modules/.pnpm/json-server-auth@2.1.0_json-server@0.17.4/node_modules/json-server-auth/dist/node_modules:/mnt/c/Users/zTTz1/Desktop/Projects/crm-auth/back/node_modules/.pnpm/json-server-auth@2.1.0_json-server@0.17.4/node_modules/json-server-auth/node_modules:/mnt/c/Users/zTTz1/Desktop/Projects/crm-auth/back/node_modules/.pnpm/json-server-auth@2.1.0_json-server@0.17.4/node_modules:/mnt/c/Users/zTTz1/Desktop/Projects/crm-auth/back/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/zTTz1/Desktop/Projects/crm-auth/back/node_modules/.pnpm/json-server-auth@2.1.0_json-server@0.17.4/node_modules/json-server-auth/dist/node_modules:/mnt/c/Users/zTTz1/Desktop/Projects/crm-auth/back/node_modules/.pnpm/json-server-auth@2.1.0_json-server@0.17.4/node_modules/json-server-auth/node_modules:/mnt/c/Users/zTTz1/Desktop/Projects/crm-auth/back/node_modules/.pnpm/json-server-auth@2.1.0_json-server@0.17.4/node_modules:/mnt/c/Users/zTTz1/Desktop/Projects/crm-auth/back/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin.js" "$@"
else
  exec node  "$basedir/../../dist/bin.js" "$@"
fi
